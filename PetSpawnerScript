-- Executor check script
local function getExecutor()
    if identifyexecutor then
        return identifyexecutor()
    elseif KRNL_LOADED then
        return "Krnl"
    elseif is_synapse_function then
        return "Synapse X"
    elseif getexecutorname then
        return getexecutorname()
    elseif DELTA_LOADED then
        return "Delta"
    else
        return "Unknown"
    end
end

local executor = getExecutor()

if executor == "Delta" then
    game:GetService("StarterGui"):SetCore("SendNotification", {
        Title = "Executor Check";
        Text = "Delta Not Supported. Please Use KRNL.";
        Duration = 5
    })
    -- Optional: Stop execution of the rest of the script
    return
end

-- Continue your script here
print("Executor is supported: " .. executor)
